    "final-form-set-field-touched": "^1.0.1",
    "focus-visible": "^5.2.0",
    "highlight-words-core": "^1.2.2",
    "jwt-decode": "^3.1.2",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "next": "^12.0.10",
    "nodemailer": "^6.7.2",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.8",
    "react-final-form-arrays": "^3.1.3",
    "react-query": "^3.34.14",
    "react-toastify": "^8.1.1",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-slug": "^7.0.1",
    "remark-stringify": "^10.0.2",
    "strip-markdown": "^5.0.0",
    "to-vfile": "^7.2.3",
    "unified": "^10.1.1",
    "vfile": "^5.3.0",
    "vfile-matter": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@jest/types": "^27.5.0",
    "@next/eslint-plugin-next": "^12.0.10",
    "@stefanprobst/commitlint-config": "^1.0.2",
    "@stefanprobst/eslint-config-next": "^5.0.2",
    "@stefanprobst/favicons": "^2.0.3",
    "@stefanprobst/prettier-config": "^2.0.1",
    "@stefanprobst/stylelint-config": "^2.1.1",
    "@stefanprobst/ts-script": "^1.0.1",
    "@stefanprobst/tsconfig": "^1.1.1",
    "@tailwindcss/typography": "^0.5.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/final-form-focus": "^1.1.2",
    "@types/final-form-set-field-data": "^1.0.0",
    "@types/final-form-set-field-touched": "^1.0.0",
    "@types/highlight-words-core": "^1.2.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/mdx": "^2.0.1",
    "@types/node": "^17.0.15",
    "@types/nodemailer": "^6.4.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.39",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "jest": "^27.5.0",
    "lint-staged": "^12.3.3",
    "msw": "^0.36.8",
    "netlify-cms-proxy-server": "^1.3.23",
    "postcss": "^8.4.6",
    "postcss-custom-media": "^8.0.0",
    "postcss-focus-visible": "^6.0.4",
    "prettier": "^2.5.1",
    "simple-git-hooks": "^2.7.0",
    "stylelint": "^14.3.0",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.5.5",
    "utility-types": "^3.10.0"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">=0.75%",
      "not dead",
      "not op_mini all"
    ]
  },
  "commitlint": {
    "extends": "@stefanprobst/commitlint-config"
  },
  "eslintConfig": {
    "extends": "@stefanprobst/eslint-config-next",
    "overrides": [
      {
        "files": [
          "src/pages/**/*.page.tsx",
          "src/pages/**/*.page.template.tsx"
        ],
        "rules": {
          "@typescript-eslint/no-namespace": "off"
        }
      }
    ],
    "ignorePatterns": "public/mockServiceWorker.js"
  },
  "lint-staged": {
    "*.(js|mjs|ts|tsx)": [
      "eslint --cache --cache-location .next/cache/eslint/ --fix",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --cache --cache-location .next/cache/stylelint/ --fix",
      "prettier --write"
    ],
    "*.(json|md|mdx)": "prettier --write"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "postcss": {
    "plugins": {
      "postcss-focus-visible": {},
      "postcss-custom-media": {},
      "tailwindcss/nesting": {},
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": "@stefanprobst/prettier-config",
  "simple-git-hooks": {
    "commit-msg": "yarn commitlint --edit",
    "pre-commit": "yarn lint-staged",
    "pre-push": "yarn run validate"
  },
  "stylelint": {
    "extends": "@stefanprobst/stylelint-config"
  }
}
