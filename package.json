{
  "name": "sshoc-marketplace-frontend",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "engines": {
    "node": "14.x",
    "yarn": "1.22.x"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "next build",
    "cms:dev": "netlify-cms-proxy-server",
    "create:favicons": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json scripts/favicons",
    "create:sshoc-client": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json scripts/sshoc-client",
    "create:sitemap": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json scripts/sitemap",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier . -l --ignore-path .gitignore",
    "format:fix": "yarn format --write",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "postbuild": "yarn create:sitemap",
    "prebuild": "rimraf .next && yarn create:sshoc-client && yarn create:favicons",
    "preexport": "rimraf out",
    "start": "next start",
    "test": "jest",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^0.2.0",
    "@hookform/devtools": "^2.2.1",
    "@hookform/resolvers": "^1.0.1",
    "@reach/accordion": "^0.11.2",
    "@reach/combobox": "^0.11.2",
    "@reach/dialog": "^0.11.2",
    "@reach/disclosure": "^0.11.2",
    "@reach/visually-hidden": "^0.11.1",
    "@stefanprobst/next-app-layout": "^1.0.1",
    "@stefanprobst/next-error-boundary": "^1.0.5",
    "@stefanprobst/next-page-metadata": "^1.0.7",
    "@stefanprobst/next-svg": "^1.0.4",
    "@stefanprobst/remark-excerpt": "^1.0.2",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.1.0",
    "clsx": "^1.1.1",
    "focus-visible": "^5.2.0",
    "jwt-decode": "^3.1.1",
    "next": "^10.0.3",
    "nodemailer": "^6.4.16",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.1",
    "postcss-focus-visible": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^6.13.1",
    "react-query": "^2.26.2",
    "react-query-devtools": "^2.6.3",
    "react-toastify": "^6.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^9.0.0",
    "strip-markdown": "^4.0.0",
    "tailwindcss": "^2.0.2",
    "unified": "^9.2.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@mdx-js/loader": "^1.6.19",
    "@next/bundle-analyzer": "^10.0.3",
    "@next/eslint-plugin-next": "^10.0.3",
    "@stefanprobst/commitlint-config": "^1.0.1",
    "@stefanprobst/eslint-config-next": "^1.0.8",
    "@stefanprobst/eslint-config-next-component": "^1.0.5",
    "@stefanprobst/favicons": "^1.0.6",
    "@stefanprobst/next-sitemap": "^1.0.4",
    "@stefanprobst/prettier-config": "^1.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/user-event": "^12.2.0",
    "@types/jest": "^26.0.15",
    "@types/jwt-decode": "^3.1.0",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.56",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jest-dom": "^3.2.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.0",
    "gray-matter": "^4.0.2",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "msw": "^0.21.3",
    "netlify-cms-proxy-server": "^1.3.6",
    "node-fetch": "^2.6.1",
    "openapi-ts-client": "git+https://github.com/stefanprobst/openapi-ts-client",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "utility-types": "^3.10.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "commitlint": {
    "extends": [
      "@stefanprobst/commitlint-config"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@stefanprobst/eslint-config-next",
      "plugin:jest-dom/recommended",
      "plugin:testing-library/react"
    ],
    "rules": {
      "@typescript-eslint/switch-exhaustiveness-check": "error"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint --cache --fix",
    "*.(css|json|md|mdx)": "prettier --write"
  }
}
