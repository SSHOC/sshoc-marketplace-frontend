    "@react-stately/collections": "^3.3.4",
    "@react-stately/combobox": "^3.0.1",
    "@react-stately/data": "^3.4.3",
    "@react-stately/menu": "^3.2.3",
    "@react-stately/numberfield": "^3.0.2",
    "@react-stately/overlays": "^3.1.3",
    "@react-stately/searchfield": "^3.1.3",
    "@react-stately/select": "^3.1.3",
    "@react-stately/toggle": "^3.2.3",
    "@react-stately/tooltip": "^3.0.5",
    "@react-stately/tree": "^3.2.0",
    "@stefanprobst/next-app-layout": "^1.0.1",
    "@stefanprobst/next-error-boundary": "^1.0.7",
    "@stefanprobst/next-page-metadata": "^1.0.7",
    "@stefanprobst/next-route-manifest": "^0.1.5",
    "@stefanprobst/next-svg": "^3.0.3",
    "@stefanprobst/remark-excerpt": "^2.0.2",
