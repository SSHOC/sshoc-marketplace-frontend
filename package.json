{
  "name": "sshoc-marketplace-frontend",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "analyze-bundle": "cross-env BUNDLE_ANALYZER='enabled' next build",
    "build": "next build",
    "cms:server": "netlify-cms-proxy-server",
    "generate:favicons": "ts-script scripts/favicons.ts",
    "dev": "next dev",
    "format": "prettier . --list-different --ignore-path .gitignore",
    "format:fix": "yarn run format --write",
    "lint:code": "eslint . --cache --cache-location .next/cache/eslint/ --ignore-path .gitignore",
    "lint:code:fix": "yarn run lint:code --fix",
    "postbuild": "yarn run generate:favicons",
    "prepare": "simple-git-hooks || true",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn run test --coverage",
    "type-check": "tsc -p ./tsconfig.json --noEmit",
    "validate": "yarn run format && yarn run lint && yarn run type-check && yarn run test"
  },
  "engines": {
    "node": "14.x||16.x",
    "yarn": "1.x"
  },
  "dependencies": {
